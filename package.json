{
  "name": "iobroker.euhome",
  "version": "0.1.1",
  "description": "Adapter for eufy Home devices like Robovac",
  "author": {
    "name": "TA2k",
    "email": "tombox2020@gmail.com"
  },
  "homepage": "https://github.com/TA2k/ioBroker.euhome",
  "license": "MIT",
  "keywords": [
    "eufy",
    "eufy home",
    "robovac"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TA2k/ioBroker.euhome.git"
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^3.1.4",
    "axios": "^1.6.8",
    "got": "^11.8.6",
    "is": "^3.3.0",
    "json2iob": "^2.6.10",
    "mqtt": "^5.5.3",
    "node-rsa": "^1.1.1",
    "randomatic": "^3.1.1",
    "sort-keys-recursive": "^2.1.10",
    "tuyapi": "^7.5.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.7.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.7.0",
    "@alcalzone/release-script-plugin-license": "^3.7.0",
    "@alcalzone/release-script-plugin-manual-review": "^3.7.0",
    "@iobroker/testing": "^4.1.3",
    "@tsconfig/node14": "^14.1.2",
    "@types/node": "^20.12.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "~5.4.5"
  },
  "main": "main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "lib/",
    "www/",
    "io-package.json",
    "LICENSE",
    "main.js"
  ],
  "scripts": {
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint .",
    "translate": "translate-adapter",
    "release": "release-script --all"
  },
  "bugs": {
    "url": "https://github.com/TA2k/ioBroker.euhome/issues"
  },
  "readmeFilename": "README.md"
}
